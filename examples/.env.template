# cribl-control-plane SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: CRIBLCONTROLPLANE_
#
# Security environment variables:
CRIBLCONTROLPLANE_BEARER_AUTH=your_bearerauth_here
CRIBLCONTROLPLANE_CLIENT_ID=your_client_id_here
CRIBLCONTROLPLANE_CLIENT_SECRET=your_client_secret_here
CRIBLCONTROLPLANE_TOKEN_URL=https://example.com/oauth/token
CRIBLCONTROLPLANE_AUDIENCE=your_audience_here

# Debug Configuration
# Enable HTTP request/response logging for debugging
CRIBLCONTROLPLANE_DEBUG=false
